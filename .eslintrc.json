{
    "parser": "babel-eslint",
    "root": true,
    "extends": "airbnb",
    "rules": {
        "max-len": [1, 100, 2],
        "semi": [2, "always"],
        "one-var": 0,
        "linebreak-style": 0,
        "indent": 0,
        "padded-blocks": 0,
        "eol-last": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "spaced-comment": 0,
        "prefer-template": 0,
        "space-before-function-paren": 0,
        "function-paren-newline": 0,
        "no-var": 0,
        "prefer-const": 0,
        "dot-notation": 0,
        "strict": 0,
        "arrow-parens": 0,
        "quote-props": 0,
        "import/no-dynamic-require": 0,
        "lines-around-directive": 0,
        "import/newline-after-import": 0,
        "prefer-destructuring": 0,
        "one-var-declaration-per-line": 0,
        "no-unused-expressions": 0,
        "object-shorthand": 0,
        "new-cap": 0,
        "max-len": 0,
        "consistent-return": 0,
        "no-param-reassign": 0,
        "comma-dangle": 0,
        "arrow-body-style": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 0,
        "no-plusplus": 0,
        "curly": ["error", "multi-line"],
        "import/no-unresolved": [2, { "commonjs": true }],
        "import/extensions": "off",
        "import/no-named-as-default": "off",
        "react/jsx-filename-extension": "off",
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "no-shadow": 0,
        "valid-jsdoc": 0,
        "require-jsdoc": 0
    },
    "globals": {
        "browser": true,
        "node": true,
        "window": true,
        "document": true,
        "Materialize": true
    },
    "env": {
        "jest": true,
        "mocha": true,
        "jquery": true,
        "commonjs": true,
        "es6": true,
        "browser": true
    }
}